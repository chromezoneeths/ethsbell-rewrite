	src="https://polyfill.io/v3/polyfill.min.js?features=Array.prototype.every,Array.prototype.filter,Array.prototype.flat,Array.prototype.includes,Array.prototype.map,ArrayBuffer,document,fetch,JSON,localStorage,Map,Number.parseFloat,Promise,Set,String.prototype.includes,String.prototype.replaceAll,Symbol.iterator,URL,WeakMap,WeakSet"
	type="module"></script>
<script src="/frontend/global/helpers.js" type="module"></script>
<meta http-equiv="Content-Security-Policy" content="default-src 'self';" />